{
  "name": "es6-workflow",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "node_modules/.bin/babel src --out-dir dist --source-maps",
    "build-watch": "node_modules/.bin/babel src --out-dir dist --source-maps --watch",
    "build-concat": "node_modules/.bin/babel src --out-file dist/index.js --source-maps",
    "serve": "node_modules/.bin/serve .",
    "dev": "npm run build-watch & npm run serve",
    "start": "npm run dev"
  },
  "author": "turing <o.u.turing@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.1.11",
    "serve": "^1.4.0"
  }
}
